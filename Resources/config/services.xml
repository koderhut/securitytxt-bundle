<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="KoderHut\SecurityTxt\SecurityTxt" class="KoderHut\SecurityTxt\SecurityTxt" public="true" lazy="true">
            <argument type="service">
                <service id="KoderHut\SecurityTxt\Format\NewLine" class="KoderHut\SecurityTxt\Format\NewLine" />
            </argument>
        </service>

    </services>
</container>
